datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model UserVerification {
  id              Int     @id @default(autoincrement())
  discordId       String? @unique // Allow null values
  discordUsername String? // Allow null values
  rainUsername    String
  verified        Boolean @default(false)
}