generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UserVerification {
  id              Int     @id @default(autoincrement())
  discordId       String? @unique
  discordUsername String?
  rainUsername    String
  verified        Boolean @default(false)
}
